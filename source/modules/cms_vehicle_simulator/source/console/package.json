    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^22.10.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-router-dom": "^5.3.3",
    "aws-amplify": "^5.3.21",
